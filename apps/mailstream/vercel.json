{
    "version": 2,
    "builds": [
      {
        "src": "./src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "installCommand": "npm install",
    "buildCommand": "npm install && cd ../../packages/db && npx prisma generate && cd ../../apps/mailstream && npx tsc -b",
    "outputDirectory": "dist"
  }