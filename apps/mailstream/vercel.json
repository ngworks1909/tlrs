{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ],
    "functions": {
      "api/*.js": {
        "memory": 512,
        "maxDuration": 10
      }
    },
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./src",
    "installCommand": "npm install",
    "buildCommand": "cd ../../packages/db && npx prisma generate && cd ../../apps/mailstream && npx tsc -b",
    "startCommand": "nodemon dist/index.js",
    "outputDirectory": "dist",
    "cleanUrls": true
  }
  