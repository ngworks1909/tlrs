
{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ],
    "buildCommand": "npm install && cd ../.. && cd packages/db && npx prisma generate && cd ../../apps/mailstream && npx tsc -b",
    "outputDirectory": "dist"
  }